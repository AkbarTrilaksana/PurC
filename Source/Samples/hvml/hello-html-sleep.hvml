#!/usr/local/bin/purc

<!DOCTYPE hvml>
<hvml target="html">
    <head>
        <title>Hello, world!</title>
    </head>

    <body>
        <ul>
            <iterate on 0 onlyif $L.lt($0<, 10) with $EJSON.arith('+', $0<, 1L) nosetotail >
                <li>$<) Hello, world! --from COROUTINE-$CRTN.cid</li>
            </iterate>
        </ul>

        <sleep for "10s" />

    </body>
</hvml>
