PREFIX=PCRWSTREAM_ERROR


Failed
    except: OSError
    flags: None
    msg: "Rwstream failed with some other error"

FileTooBig
    except: ToLarge
    flags: None
    msg: "File too large"

IO
    except: IOError
    flags: None
    msg: "IO error"

IsDir
    except: IsADirector
    flags: None
    msg: "File is a directory."

NoSpace
    except: NoSpace
    flags: None
    msg: "No space left on device."

NoDeviceOrAddress
    except: EntityNotFound
    flags: None
    msg: "No such device or address"

Overflow
    except: Overflow
    flags: None
    msg: "Value too large for defined datatype"

Pipe
    except: BrokenPipe
    flags: None
    msg: "Broken pipe"

