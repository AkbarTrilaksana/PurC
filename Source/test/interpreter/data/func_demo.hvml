<!DOCTYPE hvml SYSTEM 'v: MATH'>
<hvml target="html" lang="en">
    <head>
        <init as='foo'>
            { "id" : "foo", "name" : "bar" }
        </init>
    </head>

    <body>
        <div>
            <choose on="$foo" by="FUNC:get_member@FUNC_DEMO" with="name">
                <update on="$@" to="append" with="$?" />
            </choose>
        </div>
        <div>
            <iterate on="$foo" by="FUNC:to_array@FUNC_DEMO" with="name">
                <update on="$@" to="append" with="$?" />
            </iterate>
        </div>
    </body>
</hvml>



