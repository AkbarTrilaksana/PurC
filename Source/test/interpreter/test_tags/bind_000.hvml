<hvml target="html">
    <head>
    </head>
    <body>
        <init as="btnName" with="firstName" />
        <bind on="$btnName" as="bindName" />
        <p>The initial btnName: $btnName</p>
        <p>The current bindName: $bindName.eval</p>
        <div>
            <init as="btnName" with="secondName" temporarily />
            <p>The current bindName: $bindName.eval</p>
        </div>
    </body>
</hvml>
