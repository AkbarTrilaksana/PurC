<!DOCTYPE hvml>
<hvml target="html" lang="en">
    <head>
        <init as="users" uniquely against="id">
        [
            { "id": "c-small", "avatar": "/img/avatars/3.png", "name": "Tom",   "region": "en_US" },
            { "id": "A-BIG",   "avatar": "/img/avatars/1.png", "name": "David", "region": "en_US" },
            { "id": "b-small", "avatar": "/img/avatars/2.png", "name": "Jerry", "region": "zh_CN" },
            { "id": "D-BIG",   "avatar": "/img/avatars/4.png", "name": "David", "region": "en_US" },
            { "id": "e-small", "avatar": "/img/avatars/5.png", "name": "David", "region": "en_US" },
        ]
        </init>

    </head>

    <body>
        <sort on="$users" against="name id"/>

        <div id="calculator">
            <div>
                <div>$users[0].id</div>
                <div>$users[0].name</div>
            </div>
            <div>
                <div>$users[1].id</div>
                <div>$users[1].name</div>
            </div>
            <div>
                <div>$users[2].id</div>
                <div>$users[2].name</div>
            </div>
            <div>
                <div>$users[3].id</div>
                <div>$users[3].name</div>
            </div>
            <div>
                <div>$users[4].id</div>
                <div>$users[4].name</div>
            </div>
        </div>
    </body>

</hvml>


