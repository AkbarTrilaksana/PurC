PCHVML_TOKEN_START_TAG|<body>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_START_TAG|<header>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_START_TAG|<h1>
PCHVML_TOKEN_VCM_TREE|"Fibonacci Numbers less than 2000"
PCHVML_TOKEN_END_TAG|</h1>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_START_TAG|<p hvml:raw>
PCHVML_TOKEN_VCM_TREE|"Using local variables in two levels"
PCHVML_TOKEN_END_TAG|</p>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_END_TAG|</header>
PCHVML_TOKEN_VCM_TREE|"

        "
PCHVML_TOKEN_START_TAG|<init as="count" with="2" locally/>
PCHVML_TOKEN_VCM_TREE|"

        "
PCHVML_TOKEN_START_TAG|<section>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_START_TAG|<dl>
PCHVML_TOKEN_VCM_TREE|"
                "
PCHVML_TOKEN_START_TAG|<init as="last_one" with="0" locally/>
PCHVML_TOKEN_VCM_TREE|"
                "
PCHVML_TOKEN_START_TAG|<init as="last_two" with="1" locally/>
PCHVML_TOKEN_VCM_TREE|"

                "
PCHVML_TOKEN_START_TAG|<iterate on="1" by=concat_string("ADD: LT 2000 BY ",get_element(get_variable("!"),"last_one"))>
PCHVML_TOKEN_VCM_TREE|"
                    "
PCHVML_TOKEN_START_TAG|<init at="last_one" with=get_element(get_variable("1!"),"last_two") locally/>
PCHVML_TOKEN_VCM_TREE|"
                    "
PCHVML_TOKEN_START_TAG|<init at="last_two" with=get_variable("?") locally/>
PCHVML_TOKEN_VCM_TREE|"

                    "
PCHVML_TOKEN_START_TAG|<update on=get_variable("3!") at=".count" to="displace" with+="1"/>
PCHVML_TOKEN_VCM_TREE|"
                    "
PCHVML_TOKEN_START_TAG|<dt>
PCHVML_TOKEN_VCM_TREE|get_variable("%")
PCHVML_TOKEN_END_TAG|</dt>
PCHVML_TOKEN_VCM_TREE|"
                    "
PCHVML_TOKEN_START_TAG|<dd>
PCHVML_TOKEN_VCM_TREE|get_variable("?")
PCHVML_TOKEN_END_TAG|</dd>
PCHVML_TOKEN_VCM_TREE|"
                "
PCHVML_TOKEN_END_TAG|</iterate>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_END_TAG|</dl>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_END_TAG|</section>
PCHVML_TOKEN_VCM_TREE|"

        "
PCHVML_TOKEN_START_TAG|<footer>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_START_TAG|<p>
PCHVML_TOKEN_VCM_TREE|"Totally "
PCHVML_TOKEN_VCM_TREE|get_element(get_variable("1!"),"count")
PCHVML_TOKEN_VCM_TREE|" numbers."
PCHVML_TOKEN_END_TAG|</p>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_END_TAG|</footer>
