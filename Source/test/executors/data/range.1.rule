RANGE : FROM 0 ;
RANGE : FROM 0 TO 10 ;
RANGE : FROM 0 TO 10 , ADVANCE 2 ;
RANGE : FROM 0 TO 10 , ADVANCE 1 + 2 ;
RANGE : FROM 0 TO 10 , ADVANCE 1 + 2 / 4 - 3 * 5 ;
RANGE : FROM 0 TO 10 , ADVANCE 1 + 2 / ( 4 - 3 ) * 5 ;
# no SPACE in between
RANGE:FROM 0 TO 10,ADVANCE 1+2/(4-3)*5;
